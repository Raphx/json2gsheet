{
  "name": "json2gsheet",
  "version": "0.5.8",
  "description": "Serializes JSON data to Google Sheet, and vice versa",
  "author": "Raphx <ralcx@outlook.com>",
  "license": "MIT",
  "repository": "Raphx/json2gsheet",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "/dist",
    "/json2gsheet.config.json"
