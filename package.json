{
  "name": "json2gsheet",
  "version": "1.0.1",
  "description": "Serializes JSON data to Google Sheet, and vice versa",
  "author": "Ling Cong Xiang <ralcx@outlook.com>",
  "license": "MIT",
  "repository": "Raphx/json2gsheet",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "/dist",
    "/json2gsheet.config.json"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "babel src -d dist --delete-dir-on-start",
    "build:w": "babel src -d dist -w",
    "test": "mocha",
    "start": "node dist/cli.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "flat": "^5.0.0",
    "google-auth-library": "^6.0.5",
    "googleapis": "^54.1.0"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "standard"
  },
  "mocha": {
    "require": "@babel/register",
    "recursive": true
  }
}
